{
  "name": "acrolinx-secure-tunnel",
  "description": "Acrolinx Secure Tunnel",
  "version": "0.0.3",
  "license": "",
  "private": true,
  "main": "dist/ts/secure-tunnel.js",
  "devDependencies": {
    "@lib/autodts-generator": "~0.0.1",
    "@types/chai": "^4.1.6",
    "@types/chai-http": "^3.0.5",
    "@types/chai-string": "^1.4.1",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/mocha": "^5.2.5",
    "@types/pem": "^1.9.3",
    "@types/tmp": "0.0.33",
    "autodts": "0.0.6",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "chai-string": "^1.5.0",
    "exports-loader": "^0.7.0",
    "got": "^9.2.2",
    "isomorphic-fetch": "^2.2.1",
    "markdown-to-html": "0.0.13",
    "mocha": "^5.2.0",
    "nexe": "^2.0.0-rc.20",
    "pem": "^1.13.1",
    "replace": "^1.0.0",
    "report-viewer": "^0.3.3",
    "rimraf": "^2.6.2",
    "tmp": "0.0.33",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "xunit-file": "^1.0.0"
  },
  "scripts": {
    "clean": "rimraf dist bin",
    "prebuild": "npm run clean",
    "build": "tsc",
    "run": "tsc && node dist/ts/secure-tunnel.js",
    "tslint": "tslint -c tslint.json src/**/*.ts",
    "precompile": "npm run test && rm -rf bin && mkdir bin 2>NUL",
    "compile": "node node_modules/nexe/index.js dist/ts/secure-tunnel.js -t windows-x86-8.6.0 -n bin/secure-tunnel-windows-x86-8.6.0 && node node_modules/nexe/index.js dist/ts/secure-tunnel.js -t windows-x64-8.6.0 -n bin/secure-tunnel-windows-x64-8.6.0 && node node_modules/nexe/index.js dist/ts/secure-tunnel.js -t mac-x64-8.6.0 -n bin/secure-tunnel-mac-x64-8.6.0 && node node_modules/nexe/index.js dist/ts/secure-tunnel.js -t linux-x64-8.6.0 -n bin/secure-tunnel-linux-x64-8.6.0 && node node_modules/nexe/index.js dist/ts/secure-tunnel.js -t linux-x86-8.6.0 -n bin/secure-tunnel-linux-x86-8.6.0",
    "predebug": "npm run precompile",
    "debug": "node --nolazy --inspect=9228 --debug-brk dist/ts/secure-tunnel.js",
    "predebugWithParams": "npm run precompile",
    "debugWithParams": "node --nolazy --inspect=9230 --debug-brk dist/ts/secure-tunnel.js \"http://localhost:8031->http://test-ssl.acrolinx.com:8031\" -v -r -I",
    "doc": "node node_modules/markdown-to-html/bin/github-markdown README.md > README.html",
    "setversion": "node node_modules/replace/bin/replace.js \"666123\" \"$BUILD_NUMBER\" src/ts/secure-tunnel.cli.ts",
    "prerelease": "npm run setversion && npm run compile",
    "release": "npm run doc",
    "pretest": "npm run build",
    "test": "mocha dist/test/**/*.js --reporter xunit-file",
    "test:watch": "tsc --watch 2>&1 | mocha dist/test/**/*.js --watch 2>&1 | report-viewer --opener"
  },
  "dependencies": {
    "@types/http-proxy": "^1.16.2",
    "@types/lodash": "^4.14.117",
    "@types/opn": "^5.1.0",
    "@types/winreg": "^1.2.30",
    "commander": "^2.12.2",
    "getpass": "^0.1.7",
    "http-proxy": "^1.16.2",
    "lodash": "^4.17.4",
    "opn": "^5.1.0",
    "proxy-agent": "^3.0.3",
    "save-dev": "^2.0.0",
    "ts-node": "^7.0.1",
    "winreg": "^1.2.4"
  }
}