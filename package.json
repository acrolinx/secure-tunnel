{
  "name": "acrolinx-secure-tunnel",
  "description": "Acrolinx Secure Tunnel",
  "version": "1.1.15",
  "license": "",
  "private": true,
  "main": "dist/ts/secure-tunnel.js",
  "devDependencies": {
    "@lib/autodts-generator": "~0.0.1",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/chai-string": "^1.4.1",
    "@types/http-proxy": "^1.17.0",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/open": "^6.0.0",
    "@types/pem": "^1.9.5",
    "@types/tmp": "0.0.33",
    "@types/winreg": "^1.2.30",
    "autodts": "0.0.6",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "chai-string": "^1.5.0",
    "cross-var": "^1.1.0",
    "exports-loader": "^0.7.0",
    "got": "^9.3.2",
    "isomorphic-fetch": "^2.2.1",
    "markdown-to-html": "0.0.13",
    "mocha": "^5.2.0",
    "nexe": "^3.0.1",
    "pem": "^1.14.2",
    "replace": "^1.1.0",
    "report-viewer": "^0.3.3",
    "rimraf": "^2.6.2",
    "tmp": "0.0.33",
    "ts-loader": "^5.3.1",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1",
    "xunit-file": "^1.0.0"
  },
  "scripts": {
    "clean": "rimraf dist bin",
    "prebuild": "npm run clean && npm run tslint",
    "build": "tsc",
    "buildWatch": "tsc -w",
    "run": "tsc && node dist/ts/secure-tunnel.js \"http://localhost:7777->https://test-ssl.acrolinx.com\" -rv",
    "tslint": "tslint -p tsconfig.json \"src/**/*.ts\"",
    "tslintFix": "tslint -p tsconfig.json \"src/**/*.ts\" --fix",
    "precompile": "npm run test && rm -rf bin && mkdir bin 2>NUL",
    "compile": "cross-var node node_modules/nexe/index.js dist/ts/secure-tunnel.js -t windows-x86-8.6.0 -n bin/secure-tunnel-windows-x86-v$npm_package_version && cross-var node node_modules/nexe/index.js dist/ts/secure-tunnel.js -t windows-x64-8.6.0 -n bin/secure-tunnel-windows-x64-v$npm_package_version && cross-var node node_modules/nexe/index.js dist/ts/secure-tunnel.js -t mac-x64-8.6.0 -n bin/secure-tunnel-mac-x64-v$npm_package_version && cross-var node node_modules/nexe/index.js dist/ts/secure-tunnel.js -t linux-x64-8.6.0 -n bin/secure-tunnel-linux-x64-v$npm_package_version && cross-var node node_modules/nexe/index.js dist/ts/secure-tunnel.js -t linux-x86-8.6.0 -n bin/secure-tunnel-linux-x86-v$npm_package_version",
    "predebug": "npm run precompile",
    "debug": "node --nolazy --inspect=9228 --debug-brk dist/ts/secure-tunnel.js",
    "predebugWithParams": "npm run precompile",
    "debugWithParams": "node --nolazy --inspect=9230 --debug-brk dist/ts/secure-tunnel.js \"http://localhost:8031->https://test-ssl.acrolinx.com\" -v -r -I",
    "doc": "node node_modules/markdown-to-html/bin/github-markdown README.md > README.html",
    "setversion": "cross-var node node_modules/replace/bin/replace.js \"\\.version\\('.+?'\\)\" \".version('$npm_package_version.$TRAVIS_BUILD_NUMBER')\" src/ts/secure-tunnel.cli.ts",
    "prerelease": "npm run setversion && npm run compile",
    "release": "npm run doc",
    "pretest": "npm run build",
    "test": "mocha \"dist/test/**/*.js\" --reporter xunit-file --reporter spec",
    "test:watch": "tsc --watch 2>&1 | mocha \"dist/test/**/*.js\" --watch 2>&1 | report-viewer --opener",
    "prepushreleasetogithubandtriggeranewreleasebuild": "npm run test",
    "pushreleasetogithubandtriggeranewreleasebuild": "npm version patch -m \"v%s\"",
    "postpushreleasetogithubandtriggeranewreleasebuild": "git push && git push --tags"
  },
  "dependencies": {
    "commander": "^2.12.2",
    "getpass": "^0.1.7",
    "http-proxy": "^1.16.2",
    "lodash": "^4.17.4",
    "open": "^6.0.0",
    "proxy-agent": "^3.1.0",
    "ts-node": "^7.0.1",
    "winreg": "^1.2.4"
  }
}
